{
  "name": "windycityswing",
  "version": "0.0.1",
  "description": "Windy City Swing",
  "main": "index.js",
  "scripts": {
    "pretest": "eslint . --fix",
    "test": "jest",
    "clean": "IF EXIST archive RMDIR /S /Q archive && IF EXIST dist RMDIR /S /Q dist",
    "prestart": "npm run clean && npm run build",
    "start": "webpack-dev-server --inline --hot --content-base dist/ --host 0.0.0.0 --port 3000 --public 10.0.0.122:3000 --history-api-fallback",
    "build": "webpack -d --bail",
    "publish": "webpack -p"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^0.16.2",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "react": "^15.6.1",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.1.0",
    "babel-jest": "^9.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-polyfill": "^6.8.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.6.1",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^6.2.0",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-react": "^ 6.10.3",
    "eslint-plugin-standard": "^2.0.1",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.24.0",
    "jest": "^19.0.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "nsp": "^2.3.3",
    "prop-types": "^15.5.10",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-test-renderer": "^15.5.4",
    "resolve-url-loader": "^2.0.2",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^2.4.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dumbdimb/windycityswing.git"
  },
  "keywords": [
    "swing",
    "dancing",
    "Chicago"
  ],
  "author": "Jeremy Ma",
  "bugs": {
    "url": "https://github.com/dumbdimb/windycityswing/issues"
  },
  "homepage": "https://github.com/dumbdimb/windycityswing#readme",
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/__tests__/testUtils"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "globals": {
      "BASE_URL": true,
      "NODE_ENV": true
    }
  }
}
